@model DojoSurvey.Models.Survey
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=\, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
    crossorigin="anonymous">
    <link href="~/css/style.css">
    <title>Submitted info</title>
</head>
<body>
    <div class="container">
        <h3>Thanks for submitting!</h3>
        <div id="info">
            <p>Name: @Model.Name</p>
            <p>Dojo Location: @Model.DojoLocation</p>
            <p>Favorite Language: @Model.FavoriteLanguage</p>
            <p>Comment: @Model.Comment</p>
            <a class='btn btn-secondary' href="/">Go back</a>
        </div>
    </div>
</body>
</html>